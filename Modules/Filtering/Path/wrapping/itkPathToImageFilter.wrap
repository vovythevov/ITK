itk_wrap_class("itk::PathToImageFilter" POINTER)
  foreach(d ${ITK_WRAP_IMAGE_DIMS})
    foreach(t ${WRAP_ITK_SCALAR})
      itk_wrap_template("P${ITKM_D}${ITKM_CID${d}}${d}${ITKM_I${t}${d}}"
                        "itk::Path<${ITKT_D}, ${ITKT_CID${d}}, ${d}>, ${ITKT_I${t}${d}}")
    endforeach()
  endforeach() 
itk_end_wrap_class()
